{
  "name": "gopherhq-node",
  "version": "0.0.7-development",
  "description":
    "Node.js client for the Gopher Email API (Depricated in favor of https://www.npmjs.com/package/gopherhq)",
  "repository": {
    "type": "git",
    "url": "https://github.com/gopherhq/gopherhq-node.git"
  },
  "main": "lib/gopherhq-node.js",
  "directories": {
    "test": "test"
  },
  "files": ["lib"],
  "scripts": {
    "test": "mocha -w --require babel-register --require babel-polyfill",
    "test:inspect":
      "mocha --inspect --require babel-register --require babel-polyfill",
    "test:build":
      "REBUILD_MOCKS=true mocha --inspect --require babel-register --require babel-polyfill",
    "babel": "rimraf lib && babel --out-dir lib src"
  },
  "author": "Reilly Sweetland @rsweetland",
  "license": "MIT",
  "dependencies": {
    "axios": "0.17.1",
    "base-64": "0.1.0",
    "crypto": "0.0.3",
    "debug": "^2.6.8",
    "querystring": "^0.2.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "simple-oauth2": "^1.2.0",
    "unix-timestamp": "^0.2.0",
    "url-join": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-preset-env": "1.6.1",
    "chai": "^4.0.2",
    "dotenv": "4.0.0",
    "mocha": "^3.4.2",
    "nock": "^9.0.13",
    "node-libs-browser": "2.1.0",
    "rimraf": "2.6.2"
  }
}
