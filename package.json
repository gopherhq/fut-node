    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "simple-oauth2": "^1.2.0",
    "unix-timestamp": "^0.2.0",
    "url-join": "^2.0.2"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "6.4.1",
    "babel-preset-env": "1.6.1",
    "chai": "^4.0.2",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
